class Solution:
    def reverse(self, x: int) -> int:
        if x<0:
            sign=-1
        else:
            sign=1
        x=abs(x)
        x=list(str(x))
        n=len(x)
        low=0
        high=n-1
        while(low<=high):
            mid=(low+high)//2
            x[low],x[high]=x[high],x[low]
            low+=1
            high-=1
        x=int("".join(x))
        if x>2**31-1:
            return 0
        return sign*x
